<body onload="getcookiedata()">

    <nav>
        <ul>
            <a href="#" class="brands" id="menus" onclick="openNav()"><span
                    class="material-symbols-outlined">menu</span></a>
            <li class="brand"><img
                    src="https://t3.ftcdn.net/jpg/04/08/78/12/360_F_408781239_SioZtkPoH8lRZuL0MHG4oa2C2IyHJx0c.jpg"
                    alt="RideEasy" width=10> RideEasy</li>
            <li class="brand1"><a href="#" onclick="goToHomepage()">Home</a></li>
            <li class="brand1"><a href="#" onclick="goToAboutpage()">About</a></li>
            
            <li class="brand1">
                <div class="dropdown">
                <a href="#" class="services">Services</a>
                <a href="#" class="drop">
                    <span class="material-symbols-outlined answers">arrow_drop_down</span>
                </a>
                <div class="dropdown-content">
                    <a href="#" class="dropdown-content1"><p>Book a Car</p></a>
                    <a href="#" class="dropdown-content1"><p>Book a Bike</p></a>
                    <a href="#" class="dropdown-content1"><p>Book a Taxi</p></a>
                    <a href="#" class="dropdown-content1"><p>Book a Autorickshaw</p></a>
                </div>
            </div>
            </li>
                     
            <li class="brand1"><a href="#" id="spaces" onclick="goToContactpage()">Contact Us</a></li>
            <div class="book"><a href="#" class="Booked" onclick="goToLoginpage()">Login</a></div>
        </ul>
    </nav>
    <div id="mySidenav" class="sidenav">
        <ul>
            <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
            <li><a href="#" onclick="goToHomepage()"><span class="material-symbols-outlined">home</span>Home</a></li>
            <li><a href="#" onclick="goToAboutpage()"><span class="material-symbols-outlined">person</span> Profile</a></li>
            <li><a href="#" onclick="goToLoginpage()"><span class="material-symbols-outlined">login</span> Login</a></li>
            <li><a href="#"><span class="material-symbols-outlined">directions_car</span> Book a car</a></li>
            <li><a href="#" onclick="goToDriverLoginpage()"><span class="material-symbols-outlined">id_card</span> Driver Login</a>
            <li><a href="#" onclick="goToAboutpage()"><span class="material-symbols-outlined">info</span> About Us</a></li>
            <li><a href="#" onclick="goToContactpage()"><span class="material-symbols-outlined">contact_support</span> Contact Us</a></li>
        </ul>
    </div>
    <form action="/driverlogin" method="post">
        <div class="imgcontainer">
          <span class="material-symbols-outlined">person</span>
          <div class="importance">
            <h2>Driver Login</h2>
          </div>
        </div>
        <% if (typeof error !== 'undefined') { %>
          <p style="color: red;"><%= error %></p>
        <% } %>
        <div class="container">
          <label for="duname"><b>Username</b></label>
          <input type="text" placeholder="Enter Username" name="duname" required>
      
          <label for="dpwd"><b>Password</b></label>
          <input type="password" placeholder="Enter Password" name="dpwd" required>
      
          <button type="submit">Login</button>
          <label>
            <input type="checkbox" checked="checked" name="remember" onclick="setCookie()"> Remember me
          </label>
        </div>
      
        <div class="container">
          <button type="button" class="cancelbtn">Cancel</button>
          <span class="psw"><a href='#' onclick="goToDriverSignuppage()">New Registration</a></span>
        </div>
      </form>      
</div>
